<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <android.support.design.widget.AppBarLayout
        android:id="@+id/Bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/ButtomNav">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="E-Burjo"
                android:textColor="@android:color/white"
                android:textSize="25sp" />
        </android.support.v7.widget.Toolbar>
    </android.support.design.widget.AppBarLayout>

    </android.support.constraint.ConstraintLayout>
        <android.support.constraint.ConstraintLayout
android:layout_marginTop="55dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <android.support.design.widget.TextInputLayout
                android:layout_marginTop="20dp"
                android:id="@+id/input_layout_Nama"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:textColorHint="@color/text_color"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="30dp">

                <EditText
                    android:id="@+id/et_Nama"
                    android:layout_width="342dp"
                    android:layout_height="wrap_content"
                    android:layout_alignEnd="@+id/linearLayout5"
                    android:layout_alignLeft="@+id/luas"
                    android:layout_alignRight="@+id/linearLayout5"
                    android:layout_alignStart="@+id/luas"
                    android:layout_below="@+id/linearLayout5"
                    android:layout_marginTop="19dp"
                    android:ems="10"
                    android:hint="Masukkan Nama Warung"
                    android:inputType="text"
                    android:textColor="#094c53"
                    android:textColorHint="@color/text_color" />
            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_layout_Alamat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:gravity="center_horizontal"
                android:textColorHint="@color/text_color"
                app:layout_constraintTop_toBottomOf="@+id/input_layout_Nama"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="16dp">

                <EditText
                    android:id="@+id/et_Alamat"
                    android:layout_width="342dp"
                    android:layout_height="wrap_content"
                    android:layout_alignEnd="@+id/linearLayout5"
                    android:layout_alignLeft="@+id/luas"
                    android:layout_alignRight="@+id/linearLayout5"
                    android:layout_alignStart="@+id/luas"
                    android:layout_below="@+id/linearLayout5"
                    android:layout_marginTop="19dp"
                    android:ems="10"
                    android:hint="Masukkan Alamat Warung"
                    android:inputType="textEmailAddress"
                    android:textColor="#094c53"
                    android:textColorHint="@color/text_color" />
            </android.support.design.widget.TextInputLayout>


            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_layout_Longitude"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:gravity="center_horizontal"
                android:textColorHint="@color/text_color"
                app:layout_constraintTop_toBottomOf="@+id/input_layout_Alamat"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="16dp">

                <EditText
                    android:id="@+id/et_Longitude"
                    android:layout_width="342dp"
                    android:layout_height="wrap_content"
                    android:layout_alignEnd="@+id/linearLayout5"
                    android:layout_alignLeft="@+id/luas"
                    android:layout_alignRight="@+id/linearLayout5"
                    android:layout_alignStart="@+id/luas"
                    android:layout_below="@+id/linearLayout5"
                    android:layout_marginTop="19dp"
                    android:ems="10"
                    android:hint="Masukkan Longitude"
                    android:inputType="text"
                    android:textColor="#094c53"
                    android:textColorHint="@color/text_color" />
            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input_layout_Latitude"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:gravity="center_horizontal"
                android:textColorHint="@color/text_color"
                app:layout_constraintTop_toBottomOf="@+id/input_layout_Longitude"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="16dp">

                <EditText
                    android:id="@+id/et_Latitude"
                    android:layout_width="342dp"
                    android:layout_height="wrap_content"
                    android:layout_alignEnd="@+id/linearLayout5"
                    android:layout_alignLeft="@+id/luas"
                    android:layout_alignRight="@+id/linearLayout5"
                    android:layout_alignStart="@+id/luas"
                    android:layout_below="@+id/linearLayout5"
                    android:layout_marginTop="19dp"
                    android:ems="10"
                    android:hint="Masukkan Latitude"
                    android:inputType="text"
                    android:textColor="#094c53"
                    android:textColorHint="@color/text_color" />
            </android.support.design.widget.TextInputLayout>



            <Button
                android:id="@+id/btn_simpan"
                android:layout_width="216dp"
                android:layout_height="30dp"
                android:layout_marginBottom="50dp"
                android:layout_marginLeft="8dp"
                android:background="#379ee9"
                android:text="Simpan"
                android:textAllCaps="false"
                android:textColor="@android:color/white"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/input_layout_Latitude"
                app:layout_constraintVertical_bias="0.0" />

        </android.support.constraint.ConstraintLayout>
</FrameLayout>
